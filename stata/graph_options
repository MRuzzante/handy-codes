  
  	global slides_fontType  "Microsoft Sans Serif"
 	global  paper_fontType  "Palatino Linotype"
  
	gr set window fontface  "${paper_fontType}"
	gr set eps    fontface  "${paper_fontType}"
	
	global color0		midblue  //effect
	global color1		eltblue  //control
	global color2		navy     //treatment
	global color3		ebg      //other
	
	global graph_options    "title("") subtitle("") ylab(, angle(horizontal)) graphregion(color(white)) plotregion(color(white)) bgcolor(white)"
	
